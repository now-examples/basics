{
    "version": 2,
    "name": "nestjs-mvc",
    "alias": [
        "nestjs-mvc.now.sh"
    ],
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@now/node-server"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js"
        }
    ]
}