{
  "version": 2,
  "name": "nextjs-news",
  "builds": [
    { "src": "/static/*", "use": "@now/static" },
    { "src": "package.json", "use": "@now/next" },
    { "src": "functions/*.ts", "use": "@now/node" }
  ],
  "routes": [
    { "src": "(/.*)/", "dest": "/functions/trailing-redirect.ts" },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
