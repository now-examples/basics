{
  "version": 2,
  "name": "now-examples",
  "public": true,
  "builds": [
    { "src": "./**/package.json", "use": "@now/static-build" },
    { "src": "./**/api/**/*.sh", "use": "@now/bash" },
    { "src": "./**/api/**/*.go", "use": "@now/go" },
    { "src": "./**/api/**/*.js", "use": "@now/node" },
    { "src": "./**/api/**/*.ts", "use": "@now/node" },
    { "src": "./**/api/**/*.php", "use": "@now/php" },
    { "src": "./**/api/**/*.py", "use": "@now/python" },
    { "src": "./**/api/**/*.rb", "use": "@now/ruby" },
    { "src": "./**/api/**/*.rs", "use": "@now/rust" }
  ]
}
