    { "src": "jekyll/package.json", "use": "@now/static-build" },
    { "src": "nextjs-bash/package.json", "use": "@now/next" },
    { "src": "nextjs-go/package.json", "use": "@now/next" },
    { "src": "nextjs-node/package.json", "use": "@now/next" },
    { "src": "nextjs-php/package.json", "use": "@now/next" },
    { "src": "nextjs-python/package.json", "use": "@now/next" },
    { "src": "nextjs-ruby/package.json", "use": "@now/next" },
    { "src": "nextjs-rust/package.json", "use": "@now/next" },
    { "src": "nuxt/package.json", "use": "@now/static-build" },
    {
      "src": "umijs/package.json",
      "use": "@now/static-build",
      "config": { "distDir": "dist" }
    },
    { "src": "vue/package.json", "use": "@now/static-build" },
    { "src": "vuepress/package.json", "use": "@now/static-build" }
