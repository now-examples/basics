{
    "version": 2,
    "name": "now-examples",
    "builds": [
        { "src": "go/*.go", "use": "@now/go" },
        { "src": "nodejs/*.js", "use": "@now/node" },
        { "src": "php-7/*.php", "use": "@now/php" },
        { "src": "python/*.py", "use": "@now/python" },
        { "src": "vue/package.json", "use": "@now/static-build" },
        { "src": "static/**", "use": "@now/static" }
    ]
}