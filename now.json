  "builds": [
    { "src": "charge/package.json", "use": "@now/static-build" },
    {
      "src": "create-elm-app/package.json",
      "use": "@now/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "create-react-app/package.json",
      "use": "@now/static-build",
      "config": { "distDir": "build" }
