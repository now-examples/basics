{
  "version": 2,
  "name": "json-api",
  "alias": "json-api.now-examples.now.sh",
  "builds": [
    { "src": "www/**/*", "use": "@now/static" },
    { "src": "api/**/*.js", "use": "@now/node" }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "www/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "api/index.js"
    }
  ]
}
